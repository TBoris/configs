[user]
	name = Boris Tsema
[core]
	editor = vim
	autocrlf = input
[merge]
	tool = vimdiff
[alias]
	co = checkout
	ci = commit
	st = status
	br = branch
	hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short 
[credential]
	helper = cache --timeout=900
[color]
	status=auto
	branch=auto
	interactive=auto
	diff=auto
[push]
	default = simple
